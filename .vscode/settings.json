{
  "files.autoSave": "off",
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "\\.py$",
        "cmd": "${workspaceFolder}/.venv/Scripts/autoflake.exe ${file}"
      },
      {
        "match": "\\.py$",
        "cmd": "${workspaceFolder}/.venv/Scripts/docformatter.exe ${file}"
      },
      {
        "match": "\\.py$",
        "cmd": "${workspaceFolder}/.venv/Scripts/unexport.exe -r ${file}"
      }
    ]
  },
  "pylint.importStrategy":"fromEnvironment",
  "python.analysis.typeCheckingMode": "strict"
}
